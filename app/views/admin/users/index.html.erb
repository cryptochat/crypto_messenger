<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Пользователи</h1>
  </div>
</div>

<div class="row">
  <div class="col-lg-12">

    <div class="panel panel-default">
      <div class="panel-heading">
        Список пользователей
      </div>
      <div class="panel-body">
        <table width="100%" data-source="<%= admin_users_path(format: :json)%>"
               class="table table-striped table-bordered table-hover" id="UsersDataTables">
          <thead>
            <tr>
              <th>ID</th>
              <th>Почта</th>
              <th>Никнейм</th>
              <th>Имя</th>
              <th>Фамилия</th>
              <th>Статус</th>
              <th>Управление</th>
            </tr>
          </thead>
          <tbody></tbody>
        </table>
      </div>
    </div>
  </div>
</div>
